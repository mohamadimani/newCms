{% block scripts %}
    <script type="text/javascript">
    	$.post('public/clientside/navigationMenus', function(respose){
    		var str = '';
    		for(var i in respose){
				str += '<li><a href="public/news-group/' + respose[i]['key'] + '">' + respose[i]['value'] + '</a></li>\n';

    		}
    		$('#site-navigation-bar').append(str);
    	}, 'json');
    </script>
{% endblock %}